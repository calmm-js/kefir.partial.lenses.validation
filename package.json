{
  "name": "kefir.partial.lenses.validation",
  "version": "1.0.5",
  "description": "Partial Lenses Validation functions lifted to work on Kefir properties",
  "module": "dist/kefir.partial.lenses.validation.es.js",
  "main": "dist/kefir.partial.lenses.validation.cjs.js",
  "sideEffects": false,
  "scripts": {
    "dist": "rollup -c",
    "lint": "eslint src test",
    "prepare": "node scripts/generate.js && npm run lint && npm run dist && npm run test",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "nyc mocha",
    "watch": "concurrently \"npm run watch:dist\" \"npm run watch:test\"",
    "watch:dist": "watch-run -d 50 -i -p 'src/**/*.js' npm run dist",
    "watch:test": "watch-run -d 50    -p 'dist/kefir.partial.lenses.validation.cjs.js,test/*.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/kefir.partial.lenses.validation.git"
  },
  "keywords": [
    "kefir",
    "partial-lenses-validation",
    "reactive",
    "direct-style",
    "observable",
    "property"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/kefir.partial.lenses.validation/issues"
  },
  "homepage": "https://github.com/calmm-js/kefir.partial.lenses.validation#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-annotate-pure-calls": "^0.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.3",
    "concurrently": "^3.6.0",
    "eslint": "^5.0.1",
    "kefir": "^3.8.3",
    "kefir.ramda": "^0.25.4",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "watch-run": "^1.2.5"
  },
  "dependencies": {
    "karet.lift": "^1.1.0",
    "partial.lenses.validation": "^1.0.4"
  },
  "peerDependencies": {
    "kefir": "^3.7.0"
  }
}
